<% if user_signed_in? && current_user.likes?(link)%>
  <%= link_to "Unstink", link_like_path(link), method: :delete, remote: true %>
<% elsif user_signed_in? %>
  <%= link_to "Stink", link_like_path(link), method: :post, remote: true %>
<% else %>
  <%= link_to "Stink", link_like_path(link), method: :post %>
<% end %>

<%= link.likes.count %>

<% link.likes.each do |like| %>
  <%= like.user.avatar_default %>
<% end %>